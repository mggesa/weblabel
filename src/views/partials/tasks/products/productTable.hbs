{{#if products}}
  {{#unless encontrado}}  
    <table class="table table-bordered table-hover table-dark mt-2">
      <thead>
        <tr>
          <th>N.</th>  
          <th>Code</th>
          <th>Name</th>
          <th>Quantity</th>        
          <th>Link</th>
          <th>Label</th>
        </tr>
      </thead>
    {{#each products}}
      <tbody>
        <tr>
          <td>{{@index}}</td>  
          <td>{{Code}}</td>
          <td>{{Name}}</td>
          <td>{{Quantity}}</td>        
          <td><a href="{{Link}}" class="link-info" target="_blank">More Info</a></td>
          <td>{{Label}}</td>
        </tr>
      </tbody>
    {{/each}}
  </table>
  {{/unless}}  
{{else}}
  <div class="alert alert-danger mt-2" role="alert">
    Product not found!!
  </div>
{{/if}}
